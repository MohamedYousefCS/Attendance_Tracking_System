@model IEnumerable<Student>

@{
    ViewData["Title"] = "GetStudentByTrackID";
    Layout = "~/Views/Shared/MainLayout.cshtml";
    var currentDateAndTime = DateTime.Now.ToString("yyyy-MM-dd");


}


@section Styles {
    <style>
        #mainContainer {
            margin-top: 110px;
        }

    </style>
    <link href="~/css/attendance.css" rel="stylesheet" />

}
<div id="mainContainer">

<h2 class="head">Add Students Attendances </h2>
<div class="date-section">
    <h3>Date: <span>@currentDateAndTime</span></h3>
</div>


@{
    var viewData = new ViewDataDictionary(ViewData)
            {
                Model = Model,
                ["PropertiesToShow"] = new List<string> { "Id", "Fname", "Lname", "TrackName" }
            };
    await Html.RenderPartialAsync("~/Views/Shared/_DataTable.cshtml", Model, ViewData);

}
</div>
